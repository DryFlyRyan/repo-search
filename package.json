{
  "name": "repo-search",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "date-fns": "^2.16.1",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-select": "^3.1.0",
    "styled-components": "^5.2.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.2",
    "react-app-rewire-alias": "^0.1.6",
    "react-app-rewired": "^2.1.6"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/xformsAndMocks/fileMock.js",
      "^testUtils$": "<rootDir>/test/utils",
      "^AsyncMethods$": "<rootDir>/src/data/asyncMethods/index",
      "^Reducers$": "<rootDir>/src/data/reducers/index",
      "^Components$":  "<rootDir>/src/components/index",
      "^Subcomponents$": "<rootDir>/src/components/_subcomponents/index",
      "^Components/(.*)$": "<rootDir>/src/components/$1",
      "^GlobalStyles$": "<rootDir>/src/globalStyles",
      "^Utils$": "<rootDir>/src/utils/index"
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ]
  }
}
